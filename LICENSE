<script>
    const keyList = ["dezzzz", "jdexhhuu", "poaojajin"];

    function pickKey() {
        const randomIndex = Math.floor(Math.random() * keyList.length);
        document.getElementById("keyBox").innerText = keyList[randomIndex];
    }

    function copyKey() {
        const key = document.getElementById("keyBox").innerText;

        if (key === "Belum ada key dipilih") {
            alert("Belum ada key yang diambil!");
            return;
        }

        const temp = document.createElement("textarea");
        temp.value = key;
        document.body.appendChild(temp);

        temp.select();
        document.execCommand("copy");
        temp.remove();

        alert("âœ” Key berhasil disalin!");
    }
</script>

